@model EvaParcial.Models.EstableserEntrega

@{
    ViewData["Title"] = "Create";
}

<h2>Crear Nuevo Detalle</h2>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="idEntrega" class="control-label"></label>
                <select asp-for="idEntrega" class ="form-control" asp-items="ViewBag.idEntrega"></select>
            </div>
            <div class="form-group">
                <label asp-for="idPedidos" class="control-label"></label>
                <select asp-for="idPedidos" class ="form-control" asp-items="ViewBag.idPedidos"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
